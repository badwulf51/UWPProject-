    <!-- Comments for this are the same as they are on the cove.xaml page -->
    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}" RequestedTheme="Dark">



        <Button  Content="Back" Click="Button_Click" HorizontalAlignment="Left" Margin="0,10,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.782,-0.793"/>

        <TextBlock Margin="62,58,12,670" FontSize="10">
    <Run Text="The Weald is the second dungeon that can be explored. It is a sprawling forest" />
    <LineBreak />
    <Run Text="whose rotted boughs host a hideous kind of fungi. The only roads to the Hamlet" />
    <LineBreak />
    <Run Text="pass through the diseased wood, making it an important area to cleanse." />
        </TextBlock>

        <TextBlock Margin="62,113,12,615" FontSize="10">
    <Run Text="Weakness: Stress healers such as the jester make folly of this area!" Foreground="#FF11C711" />
    <LineBreak />
    <Run Text="Beware: Massive orges lurk on longer dungeons, there power is hainous!" Foreground="#FFD60E0E" />
    <LineBreak />
    <Run Text="Allies: Jester, Bounty Hunter, Vestal, Highwayman " />
        </TextBlock>


